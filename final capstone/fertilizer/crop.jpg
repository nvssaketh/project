		$('.lang-mobile').toggleClass('lang-dropdown-active');
	})
})
$(window).on('load',function(){
	const url = window.location.href;
	const urlHttp = url.split(':');
	//console.log(urlHttp[1]);
	const languageSwitch = $('.language-switch a');
	languageSwitch.removeClass('active_cls');
	languageSwitch.each(function(){
		const hrefUrl = $(this).attr('href');
		const urlCondi = urlHttp[1] == hrefUrl;
		const switcherValue = $(this).text(); 
		const languageSwitcher = () => {
			$(this).addClass('active_cls');
			$('.language-ico').text(switcherValue);
		}
		urlCondi ? languageSwitcher() : ''
	})
});

